<?xml version="1.0" encoding="utf-8"?>
<Defs>
	<XmlExtensions.SettingsMenuDef>
		<defName>ferriteweaveSettings</defName> <!--Can be anything unique with no periods-->
		<modId>monopoda.ferriteweave</modId> <!--MUST be the same as the one entered earlier-->    
		<label>Ferriteweave</label> <!-- This is what displays as your mod's name-->    
		<settings> <!--This is where you define what settings are in the settings menu-->
			<li Class="XmlExtensions.Setting.Textbox"> <!--Can use any class from XmlExtensions.Setting-->
				<key>commonality</key>
			<label>Commonality</label>
			<defaultValue>0.22</defaultValue> <!--Can generally skip, but good to include again to avoid possible bugs-->
		</li>
		</settings>
	</XmlExtensions.SettingsMenuDef>
</Defs>